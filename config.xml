<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tradingloglite" version="1.0.0" versionCode="100000" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>TradingLog Lite</name>
	<description>
		A mobile app designed to keep track of your trading activities and ease money management by calculating your stats.
	</description>
	<author email="info@tradinglog.com" href="http://tradinglog.com/">
		TradingLog
	</author>
	<content src="index.html" />

	<gap:platform name="android" />
	<gap:platform name="ios" />

	<preference name="android-installLocation" value="auto" />
	<preference name="applicationSupportsShakeToEdit" value="no" />
	<preference name="auto-hide-splash-screen" value="false" />
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="BackupWebStorage" value="none" />
	<preference name="detect-data-types" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="exit-on-suspend" value="false" />
	<preference name="FadeSplashScreen" value="false" />
	<preference name="fullscreen" value="true" />
	<preference name="HideKeyboardFormAccessoryBar" value="true" />
	<preference name="KeyboardDisplayRequiresUserAction" value="false" />
	<preference name="KeyboardShrinksView" value="true" />
	<preference name="orientation" value="portrait" />
	<preference name="permissions" value="none" />
	<preference name="phonegap-version" value="3.7.0" />
	<preference name="prerendered-icon" value="true" />
	<preference name="show-splash-screen-spinner" value="false" />
	<preference name="ShowSplashScreenSpinner" value="false" />
	<preference name="stay-in-webview" value="false" />
	<preference name="target-device" value="universal" />
	<preference name="webviewbounce" value="false" />

	<gap:config-file mode="replace" parent="UISupportedInterfaceOrientations" platform="ios">
		<array>
			<string>UIInterfaceOrientationLandscapeOmg</string>
		</array>
	</gap:config-file>

	<gap:plugin name="com.brodysoft.sqliteplugin" />
	<gap:plugin name="com.ionic.keyboard" />
	<gap:plugin name="com.jchalan.cordova.ios.disableshaketoundo" />
	<gap:plugin name="com.plugin.datepicker" />
	<gap:plugin name="org.apache.cordova.device" />
	<gap:plugin name="org.apache.cordova.device-motion" />
	<gap:plugin name="org.apache.cordova.inappbrowser" />
	<gap:plugin name="org.apache.cordova.network-information" />
	<gap:plugin name="org.apache.cordova.splashscreen" />
	<gap:plugin name="org.apache.cordova.statusbar" />

	<icon src="icon.png" />
	<icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
	<icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
	<icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
	<icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
	<icon gap:platform="ios" height="100" src="res/icon/ios/icon-small-50-2x.png" width="100" />
	<icon gap:platform="ios" height="114" src="res/icon/ios/icon-57-2x.png" width="114" />
	<icon gap:platform="ios" height="120" src="res/icon/ios/icon-60-2x.png" width="120" />
	<icon gap:platform="ios" height="144" src="res/icon/ios/icon-72-2x.png" width="144" />
	<icon gap:platform="ios" height="152" src="res/icon/ios/icon-76-2x.png" width="152" />
	<icon gap:platform="ios" height="180" src="res/icon/ios/icon-60-3x.png" width="180" />
	<icon gap:platform="ios" height="29" src="res/icon/ios/icon-small.png" width="29" />
	<icon gap:platform="ios" height="40" src="res/icon/ios/icon-small-40.png" width="40" />
	<icon gap:platform="ios" height="50" src="res/icon/ios/icon-small-50.png" width="50" />
	<icon gap:platform="ios" height="57" src="res/icon/ios/icon-57.png" width="57" />
	<icon gap:platform="ios" height="58" src="res/icon/ios/icon-small-2x.png" width="58" />
	<icon gap:platform="ios" height="60" src="res/icon/ios/icon-60.png" width="60" />
	<icon gap:platform="ios" height="72" src="res/icon/ios/icon-72.png" width="72" />
	<icon gap:platform="ios" height="76" src="res/icon/ios/icon-76.png" width="76" />
	<icon gap:platform="ios" height="80" src="res/icon/ios/icon-small-40-2x.png" width="80" />
	<icon gap:platform="ios" height="87" src="res/icon/ios/icon-small-3x.png" width="87" />

	<gap:splash src="splash.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
	<gap:splash gap:platform="ios" height="1024" src="res/screen/ios/screen-ipad-portrait.png" width="768" />
	<gap:splash gap:platform="ios" height="1136" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
	<gap:splash gap:platform="ios" height="1334" src="res/screen/ios/screen-iphone-portrait-667h-2x.png" width="750" />
	<gap:splash gap:platform="ios" height="2048" src="res/screen/ios/screen-ipad-portrait-2x.png" width="1536" />
	<gap:splash gap:platform="ios" height="2208" src="res/screen/ios/screen-iphone-portrait-736h-3x.png" width="1242" />
	<gap:splash gap:platform="ios" height="480" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
	<gap:splash gap:platform="ios" height="960" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />

	<access origin="*" />
</widget>
