<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tradinglog" versionCode="100002" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>TradingLog</name>
	<description>
		A mobile app designed to keep track of your trading activities and ease money management by calculating your stats.
	</description>
	<author email="info@tradinglog.com" href="http://tradinglog.com/">
		TradingLog
	</author>

	<gap:platform name="android" />
	<gap:platform name="ios" />

	<content src="index.dist.html" />

	<preference name="android-installLocation" value="auto" />
	<preference name="applicationSupportsShakeToEdit" value="no" />
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="BackupWebStorage" value="none" />
	<preference name="detect-data-types" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="FadeSplashScreen" value="false" />
	<preference name="fullscreen" value="true" />
	<preference name="HideKeyboardFormAccessoryBar" value="true" />
	<preference name="KeyboardDisplayRequiresUserAction" value="false" />
	<preference name="KeyboardShrinksView" value="true" />
	<preference name="orientation" value="portrait" />
	<preference name="permissions" value="none" />
	<preference name="phonegap-version" value="3.7.0" />
	<preference name="prerendered-icon" value="true" />
	<preference name="show-splash-screen-spinner" value="false" />
	<preference name="ShowSplashScreenSpinner" value="false" />
	<preference name="target-device" value="universal" />
	<preference name="webviewbounce" value="false" />

	<gap:config-file platform="ios" parent="UISupportedInterfaceOrientations" mode="replace">
		<array>
			<string>UIInterfaceOrientationLandscapeOmg</string>
		</array>
	</gap:config-file>

	<gap:plugin name="com.brodysoft.sqliteplugin" />
	<gap:plugin name="com.ionic.keyboard" />
	<gap:plugin name="com.jchalan.cordova.ios.disableshaketoundo" />
	<gap:plugin name="com.plugin.datepicker" />
	<gap:plugin name="org.apache.cordova.device" />
	<gap:plugin name="org.apache.cordova.device-motion" />
	<gap:plugin name="org.apache.cordova.inappbrowser" />
	<gap:plugin name="org.apache.cordova.network-information" />
	<gap:plugin name="org.apache.cordova.splashscreen" />
	<gap:plugin name="org.apache.cordova.statusbar" />

	<icon gap:platform="android" gap:qualifier="hdpi" src="img/res/android/icon/icon-72-hdpi.png" />
	<icon gap:platform="android" gap:qualifier="ldpi" src="img/res/android/icon/icon-36-ldpi.png" />
	<icon gap:platform="android" gap:qualifier="mdpi" src="img/res/android/icon/icon-48-mdpi.png" />
	<icon gap:platform="android" gap:qualifier="xhdpi" src="img/res/android/icon/icon-96-xhdpi.png" />
	<icon gap:platform="ios" height="100" src="img/res/ios/icon/icon-50@2x.png" width="100" />
	<icon gap:platform="ios" height="114" src="img/res/ios/icon/icon-57@2x.png" width="114" />
	<icon gap:platform="ios" height="120" src="img/res/ios/icon/icon-60@2x.png" width="120" />
	<icon gap:platform="ios" height="144" src="img/res/ios/icon/icon-72@2x.png" width="144" />
	<icon gap:platform="ios" height="152" src="img/res/ios/icon/icon-76@2x.png" width="152" />
	<icon gap:platform="ios" height="180" src="img/res/ios/icon/icon-60@3x.png" width="180" />
	<icon gap:platform="ios" height="29" src="img/res/ios/icon/icon-29.png" width="29" />
	<icon gap:platform="ios" height="40" src="img/res/ios/icon/icon-40.png" width="40" />
	<icon gap:platform="ios" height="50" src="img/res/ios/icon/icon-50.png" width="50" />
	<icon gap:platform="ios" height="57" src="img/res/ios/icon/icon-57.png" width="57" />
	<icon gap:platform="ios" height="58" src="img/res/ios/icon/icon-29@2x.png" width="58" />
	<icon gap:platform="ios" height="60" src="img/res/ios/icon/icon-60.png" width="60" />
	<icon gap:platform="ios" height="72" src="img/res/ios/icon/icon-72.png" width="72" />
	<icon gap:platform="ios" height="76" src="img/res/ios/icon/icon-76.png" width="76" />
	<icon gap:platform="ios" height="80" src="img/res/ios/icon/icon-40@2x.png" width="80" />
	<icon src="icon.png" />

	<gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="img/res/android/splash/screen-hdpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="img/res/android/splash/screen-ldpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="img/res/android/splash/screen-mdpi-portrait.png" />
	<gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="img/res/android/splash/screen-xhdpi-portrait.png" />
	<gap:splash gap:platform="ios" height="1024" src="img/res/ios/splash/splash-ipad.png" width="768" />
	<gap:splash gap:platform="ios" height="1136" src="img/res/ios/splash/splash-568h@2x.png" width="640" />
	<gap:splash gap:platform="ios" height="1334" src="img/res/ios/splash/splash-667h.png" width="750" />
	<gap:splash gap:platform="ios" height="2048" src="img/res/ios/splash/splash-ipad@2x.png" width="1536" />
	<gap:splash gap:platform="ios" height="2208" src="img/res/ios/splash/splash-736h.png" width="1242" />
	<gap:splash gap:platform="ios" height="480" src="img/res/ios/splash/splash-iphone.png" width="320" />
	<gap:splash gap:platform="ios" height="960" src="img/res/ios/splash/splash-iphone@2x.png" width="640" />
	<gap:splash src="splash.png" />

	<access origin="*" />
</widget>
