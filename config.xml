<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tradinglog" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>TradingLog</name>
	<description>
		A mobile app designed to keep track of your trading activities and ease money management by calculating your stats.
	</description>
	<author email="info@tradinglog.com" href="http://tradinglog.com">
		TradingLog Team
	</author>

	<gap:platform name="ios" />

	<content src="index.html" />

	<preference name="detect-data-types" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="fullscreen" value="true" />
	<preference name="orientation" value="portrait" />
	<preference name="phonegap-version" value="3.6.3" />
	<preference name="prerendered-icon" value="true" />
	<preference name="target-device" value="universal" />
	<preference name="webviewbounce" value="false" />

	<gap:config-file platform="ios" parent="UISupportedInterfaceOrientations" mode="replace">
		<array>
			<string>UIInterfaceOrientationLandscapeOmg</string>
		</array>
	</gap:config-file>

	<gap:plugin name="org.apache.cordova.battery-status" />
	<gap:plugin name="org.apache.cordova.camera" />
	<gap:plugin name="org.apache.cordova.console" />
	<gap:plugin name="org.apache.cordova.contacts" />
	<gap:plugin name="org.apache.cordova.device" />
	<gap:plugin name="org.apache.cordova.device-motion" />
	<gap:plugin name="org.apache.cordova.device-orientation" />
	<gap:plugin name="org.apache.cordova.dialogs" />
	<gap:plugin name="org.apache.cordova.file" />
	<gap:plugin name="org.apache.cordova.file-transfer" />
	<gap:plugin name="org.apache.cordova.geolocation" />
	<gap:plugin name="org.apache.cordova.globalization" />
	<gap:plugin name="org.apache.cordova.inappbrowser" />
	<gap:plugin name="org.apache.cordova.media" />
	<gap:plugin name="org.apache.cordova.media-capture" />
	<gap:plugin name="org.apache.cordova.network-information" />
	<gap:plugin name="org.apache.cordova.vibration" />

	<icon src="img/res/ios/icon/icon-29.png" gap:platform="ios" width="29" height="29" />
	<icon src="img/res/ios/icon/icon-29@2x.png" gap:platform="ios" width="58" height="58" />
	<icon src="img/res/ios/icon/icon-40.png" gap:platform="ios" width="40" height="40" />
	<icon src="img/res/ios/icon/icon-40@2x.png" gap:platform="ios" width="80" height="80" />
	<icon src="img/res/ios/icon/icon-50.png" gap:platform="ios" width="50" height="50" />
	<icon src="img/res/ios/icon/icon-50@2x.png" gap:platform="ios" width="100" height="100" />
	<icon src="img/res/ios/icon/icon-57.png" gap:platform="ios" width="57" height="57" />
	<icon src="img/res/ios/icon/icon-57@2x.png" gap:platform="ios" width="114" height="114" />
	<icon src="img/res/ios/icon/icon-60.png" gap:platform="ios" width="60" height="60" />
	<icon src="img/res/ios/icon/icon-60@2x.png" gap:platform="ios" width="120" height="120" />
	<icon src="img/res/ios/icon/icon-60@3x.png" gap:platform="ios" width="180" height="180" />
	<icon src="img/res/ios/icon/icon-72.png" gap:platform="ios" width="72" height="72" />
	<icon src="img/res/ios/icon/icon-72@2x.png" gap:platform="ios" width="144" height="144" />
	<icon src="img/res/ios/icon/icon-76.png" gap:platform="ios" width="76" height="76" />
	<icon src="img/res/ios/icon/icon-76@2x.png" gap:platform="ios" width="152" height="152" />
	<icon src="icon.png" />

	<access origin="*" />
</widget>
